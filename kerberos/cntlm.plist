<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>KeepAlive</key>
        <true />
        <key>Label</key>
        <string>cntlm.kerberos.listener</string>
        <key>RunAtLoad</key>
        <true />
        <key>ProgramArguments</key>
        <array>
            <string>{PATH}/listener.swift</string>
            <string>-start</string>
            <string>{PATH}/cntlm.sh start</string>
            <string>-notifications</string>
            <string>com.apple.KerberosPlugin.InternalNetworkAvailable</string>
            <string>com.apple.KerberosPlugin.InternalNetworkNotAvailable</string>
            <string>-actions</string>
            <string>{PATH}/cntlm.sh internal</string>
            <string>{PATH}/cntlm.sh external</string>
        </array>
    </dict>
</plist>